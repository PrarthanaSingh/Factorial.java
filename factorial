package series;

import java.util.Scanner;

public class factorial {
        public static void main(String args[])throws Exception{
        	
        	Scanner s =new Scanner(System.in);
        	System.out.println("Enter the number");
        	int a = s.nextInt();
        	long b=1L;
        	int i;
        	 
        		for(i=1;i<=a;i++)
        		{
                 b= b*i;
        		}
        		System.out.println(b+"");
        	
        }
}
